# ============================================================================
# JupyterHub Environment Configuration
# ============================================================================
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file to version control!

# ----------------------------------------------------------------------------
# JupyterHub Security
# ----------------------------------------------------------------------------
# Generate a random key with: openssl rand -hex 32
JUPYTERHUB_CRYPT_KEY=CHANGE_ME_GENERATE_RANDOM_HEX_KEY

# ----------------------------------------------------------------------------
# Cloudflare Tunnel Configuration
# ----------------------------------------------------------------------------
# Get your tunnel token from: https://dash.cloudflare.com
# 1. Go to Zero Trust > Networks > Tunnels
# 2. Create a new tunnel
# 3. Copy the token provided
CLOUDFLARE_TUNNEL_TOKEN=YOUR_CLOUDFLARE_TUNNEL_TOKEN_HERE

# ----------------------------------------------------------------------------
# Docker Configuration
# ----------------------------------------------------------------------------
DOCKER_NETWORK_NAME=jupyterhub-network

# ----------------------------------------------------------------------------
# Optional: Logging
# ----------------------------------------------------------------------------
# Options: DEBUG, INFO, WARNING, ERROR, CRITICAL
JUPYTERHUB_LOG_LEVEL=INFO
